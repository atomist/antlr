{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "max-line-length": [true, 150]
    },
    "rulesDirectory": []
}
